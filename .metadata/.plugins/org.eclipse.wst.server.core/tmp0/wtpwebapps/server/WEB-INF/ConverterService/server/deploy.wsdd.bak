<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ConverterService WSDL service -->

  <service name="Converter" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://server"/>
      <parameter name="wsdlServiceElement" value="ConverterService"/>
      <parameter name="schemaQualified" value="http://server"/>
      <parameter name="wsdlServicePort" value="Converter"/>
      <parameter name="className" value="server.ConverterSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Converter"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="fahrenheitToCelsius" qname="operNS:fahrenheitToCelsius" xmlns:operNS="http://server" returnQName="retNS:fahrenheitToCelsiusReturn" xmlns:retNS="http://server" returnType="rtns:float" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:fahrenheit" xmlns:pns="http://server" type="tns:float" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="celsiusToFahrenheit" qname="operNS:celsiusToFahrenheit" xmlns:operNS="http://server" returnQName="retNS:celsiusToFahrenheitReturn" xmlns:retNS="http://server" returnType="rtns:float" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:celsius" xmlns:pns="http://server" type="tns:float" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="celsiusToFahrenheit fahrenheitToCelsius"/>

  </service>
</deployment>
